{
  "name": "test-for-super-job",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://dmitruvasilev.github.io/test_for_SuperJob/",
  "scripts": {
    "start": "react-app-rewired start",
    "build": "react-app-rewired build",
    "test": "react-app-rewired test --env=jsdom",
    "eject": "react-app-rewired eject",
    "lint": "eslint \"src/**/*.js\"",
    "lint:fix": "npm run lint -- --fix",
    "format": "prettier-eslint --write \"**/*.{js,json}\"",
    "deploy": "npm run build && gh-pages -d build -m \"Updated at $(date +'%a, %d %b %Y, %T, %Z')\" --add"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "**/*.js": [
      "npm run lint:fix",
      "npm run format",
      "git add"
    ],
    "**/*.json": [
      "npm run format",
      "git add"
    ]
  },
  "dependencies": {
    "classnames": "^2.2.6",
    "latest": "^0.2.0",
    "normalize.css": "^8.0.1",
    "prop-types": "^15.6.1",
    "react": "^16.7.0",
    "react-app-rewire-eslint": "^0.2.3",
    "react-app-rewire-polyfills": "^0.2.0",
    "react-app-rewire-scss": "^1.0.2",
    "react-app-rewired": "^1.5.0",
    "react-dom": "^16.7.0",
    "react-scripts": "^1.1.1"
  },
  "devDependencies": {
    "babel-eslint": "^8.2.6",
    "eslint": "^4.10.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-react": "^7.12.0",
    "gh-pages": "^1.2.0",
    "husky": "^1.3.1",
    "lint-staged": "^7.0.0",
    "prettier-eslint-cli": "^4.7.1",
    "react-test-renderer": "^16.2.0"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
